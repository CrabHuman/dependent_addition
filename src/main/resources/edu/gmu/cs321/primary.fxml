<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.gmu.cs321.PrimaryController">
   <children>
      <Label text="Primary View" />
      <Button fx:id="dataEntryButton" text="Switch to Data Entry" onAction="#switchToDataEntry"/>
      <Button fx:id="reviewButton" text="Switch to Review" onAction="#switchToReview"/>
      <Button fx:id="approvalButton" text="Switch to Approval" onAction="#switchToApproval"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
